{
  "name": "@142vip/egg-grpc-server",
  "version": "0.0.2",
  "description": "egg.js框架下grpc服务端插件，支持grpc服务远程调用",
  "eggPlugin": {
    "name": "grpcServer"
  },
  "main": "index.js",
  "keywords": [
    "公众号：Rong姐姐好可爱",
    "142vip",
    "grpc",
    "egg-grpc",
    "egg.js"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.13"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg": "^2.16.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
